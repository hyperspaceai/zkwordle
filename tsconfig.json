{
  "extends": "@hyperspaceinc/style-guide/tsconfig/next",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "strict": false,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx"
  ]
}
